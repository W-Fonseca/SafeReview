      Title="Page_Informacoes">

    <TextBlock x:Name="informacoes_projeto" Text="{DynamicResource Informacoes_txtB_informacoes}" xml:space="preserve" Foreground="White" FontSize="14" TextWrapping="WrapWithOverflow" MouseLeftButtonDown="AjusteImagem">

